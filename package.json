{
  "name": "middleware-graphql",
  "version": "0.0.9",
  "license": "ICS",
  "main": "lib/server/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node\" src/server/server.ts",
    "start-prod": "NODE_ENV=production node lib/src/server/server.js",
    "test": "npm run test:unit && npm run lint && npm run test:api",
    "test:unit": "cross-env TEST_SUITE=unit ts-node --O '{ \"rootDir\": \".\" }' node_modules/istanbul/lib/cli.js cover -e .ts tests/jasmine-runner.ts && istanbul report cobertura",
    "test:api": "cross-env TEST_SUITE=api ts-node --O '{ \"rootDir\": \".\" }' tests/jasmine-runner.ts",
    "test:governor": "cross-env TEST_SUITE=governor ts-node --O '{ \"rootDir\": \".\" }' tests/jasmine-runner.ts",
    "lint": "tslint --type-check --project tsconfig.json \"src/**/*.ts\" -e \"node_modules/**/*\" -t stylish",
    "build": "npm ci && rimraf ./lib/ && node_modules/typescript/bin/tsc && node node_modules/cpx/bin/index.js \"src/**/*.json\" \"lib/src\""
  },
  "dependencies": {
    "@connections/cachy-service": "0.0.39",
    "@connections/orient-mock-data": "^0.4.53",
    "@connections/utils-auth": "^1.1.23",
    "@connections/utils-auth-web": "^0.2.153",
    "@connections/utils-gatekeeper": "^0.2.153",
    "@connections/utils-ic-config": "^0.2.153",
    "@connections/utils-logger": "^3.1.6",
    "@connections/utils-orient-api": "^0.2.153",
    "@connections/utils-orient-server": "^0.2.153",
    "casual": "^1.6.2",
    "composable-middleware": "0.3.0",
    "compression": "1.7.1",
    "cookie-parser": "1.4.1",
    "cors": "2.5.2",
    "cross-env": "1.0.7",
    "cross-fetch": "3.1.5",
    "es6-promise": "3.3.1",
    "graphql": "0.13.2",
    "graphql-server-express": "1.4.0",
    "graphql-subscriptions": "1.0.0",
    "graphql-tools": "3.0.5",
    "helmet": "^3.23.3",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "loopback": "^3.28.0",
    "loopback-boot": "^3.3.1",
    "node-fetch": "2.6.7",
    "q": "1.4.1",
    "serve-favicon": "2.4.5",
    "strong-error-handler": "^4.0.0"
  },
  "devDependencies": {
    "@connections/utils-graphql-queries": "^0.2.153",
    "@types/express": "4.0.35",
    "@types/graphql": "0.13.2",
    "@types/jasmine": "2.5.47",
    "@types/lodash": "^4.14.182",
    "@types/node": "14.14.6",
    "cpx": "1.5.0",
    "istanbul": "1.0.0-alpha.2",
    "jasmine": "2.6.0",
    "jasmine-spec-reporter": "4.1.0",
    "nodemon": "^1.19.4",
    "react": "15.3.2",
    "rewire": "2.3.4",
    "rimraf": "2.5.4",
    "ts-node": "3.1.0",
    "tslint": "^5.20.1",
    "typescript": "4.9.4"
  },
  "optionalDependencies": {
    "loopback-component-explorer": "2.4.0"
  }
}